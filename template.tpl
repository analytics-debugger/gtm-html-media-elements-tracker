// https://www.analytics-debugger.com
// https://www.thyngster.com

const log = require('logToConsole');
const injectScript = require('injectScript');
const callLater = require('callLater');
const callInWindow = require('callInWindow');

const config = {
  tms: 'gtm',
  datalayerVariableNames: ['auto'],
  debug: data.advanced_debug_mode || false,
  observe: data.advanced_observe || false,
  data_elements: data.advanced_data_elements || false, 
  start: data.actions_start || false,
  play: data.actions_play || false,
  pause: data.actions_pause || false,
  mute: data.actions_mute || false,
  unmute: data.actions_unmute || false,
  complete: data.actions_complete || false,
  seek: data.actions_seek || false,
  progress: data.actions_progress || false,
  progress_tracking_method: 'percentages',
  progress_percentages: data.progress_percentages ? data.progress_percentages.split(',').map(function(e){ return e.trim(); }) : undefined,
  progress_thresholds: undefined
};


const templateLibrary = {
  init: () => {
    log('templateLibrary.init', config);
    data.gtmOnSuccess();  
    callLater(function(){            
        callInWindow("_htmlMediaElementsTracker.init", config);
     });        
  }
};
 
const url = 'https://cdn.jsdelivr.net/npm/@analytics-debugger/html-media-elements@latest/dist/htmlMediaElementsTracker.min.js';             
injectScript(url, templateLibrary.init, data.gtmOnFailure, url);


